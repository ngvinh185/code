#include <bits/stdc++.h>
#define vinh ios::sync_with_stdio(false);
#define nguyen cin.tie(nullptr);
#define anonymous main
#define int long long
#define for(i,a,b) for(int i=a;i<b;i++)
#define For(i,a,b) for(int i=a;i>=b;i--)

using namespace std;
anonymous()
{
	vinh nguyen
	int n;cin>>n;
	int k;cin>>k;
	int a[n];
	for(i,0,n) cin>>a[i];
	sort(a,a+n);
	int l=0,r=n-1;
	while(l<r)
	{
		if(a[l]+a[r]==k) 
		{
			cout<<"YES";
			return 0;
		}
		else if (a[l]+a[r]>k) r-=1;
		else l+=1;
	}
	cout<<"NO";
}
